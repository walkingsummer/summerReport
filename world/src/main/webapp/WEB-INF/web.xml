<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	  
	<filter>
		<filter-name>ReportUserLoginFilter</filter-name>
		<filter-class>filter.ReportUserLoginFilter</filter-class>
	</filter>
	
	<filter-mapping>
		<filter-name>ReportUserLoginFilter</filter-name>
		<url-pattern>/jsp/generalReport/*</url-pattern>
	</filter-mapping>
  	
  	<filter-mapping>
  		<filter-name>ReportUserLoginFilter</filter-name>
		<url-pattern>/jsp/userRetainedReport/*</url-pattern>
  	</filter-mapping>
  	
  	<filter-mapping>
  		<filter-name>ReportUserLoginFilter</filter-name>
  		<url-pattern>/jsp/usage/*</url-pattern>
  	</filter-mapping>
  	
  	<filter-mapping>
  		<filter-name>ReportUserLoginFilter</filter-name>
  		<url-pattern>/jsp/userRetainedReport/*</url-pattern>
  	</filter-mapping>
  	
  	<servlet>
  		<servlet-name>GeneralReportServlet</servlet-name>
  		<servlet-class>servlet.GeneralReportServlet</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>GeneralReportServlet</servlet-name>
  		<url-pattern>/jsp/generalReport/generalReportTable.jsp</url-pattern>
  	</servlet-mapping>
  
</web-app>


